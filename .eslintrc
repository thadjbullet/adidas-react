{
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/prop-types": 0,
    "jsx-a11y/href-no-hash": 0,
    "no-unused-vars": 0,
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  }
}
